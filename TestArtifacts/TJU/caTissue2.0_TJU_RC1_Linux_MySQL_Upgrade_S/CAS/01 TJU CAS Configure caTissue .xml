<?xml version="1.0" encoding="UTF-8"?>

<IDPS>
	<IDP name="CSM">
		<class name="edu.wustl.authmanager.CSMAuthManager" />
		<idpdetails>
			<defaultidp>true</defaultidp>
			<displayname>CSM</displayname>
		</idpdetails>
		<idp-configuration>
		</idp-configuration>
        	<user-attributes>
		</user-attributes>
		<migration-details>
			<migrator>
 			    <rule-class>edu.wustl.migration.rules.CAGridMigrationRule</rule-class>
			    <migrator-class>edu.wustl.migrator.CAGridMigrator</migrator-class>
			    <target-idp-name>caGridDorianIDP</target-idp-name>
			</migrator>
			 <migrator>
				<rule-class>edu.wustl.migration.rules.CAGridMigrationRule</rule-class>
				<migrator-class>edu.wustl.migrator.CAGridMigrator</migrator-class>
				<target-idp-name>TJU_IDP</target-idp-name>
			</migrator>
		</migration-details>
	</IDP>
	<IDP name="TJU_IDP">
		<class name="edu.wustl.authmanager.LDAPAuthManager" />
		<idpdetails>
		<defaultidp>false</defaultidp>
		<displayname>TJULDAP</displayname>
		</idpdetails>
		<idp-configuration>
			<initContextFactory>com.sun.jndi.ldap.LdapCtxFactory</initContextFactory>
			<securityAuthetication>simple</securityAuthetication>
			<securityProtocol></securityProtocol>
			<directory>ldap://jds.jefferson.edu:389/</directory>
			<searchbase>ou=people,dc=jefferson,dc=edu</searchbase>
			<searchfilter>uid=name</searchfilter>
			<stringToReplace>name</stringToReplace>
			<bindUser>ou=people,dc=jefferson,dc=edu</bindUser>
			<bindPassword></bindPassword>
		</idp-configuration>
		 <error-codes>
				<error code="525" message="User not found" />
				<error code="52e" message="Invalid credentials" />
				<error code="530" message="Not permitted to logon at this time" />
				<error code="532" message="Password expired,contact system administrator" />
				<error code="533" message="Account disabled,contact system administrator" />
				<error code="773" message="User must reset password batch update,contact system administrator" />
				<error code="701" message="Account expired, contact system administrator" />
		</error-codes>        
		<user-attributes>
				<firstName>givenname</firstName>
				<lastName>sn</lastName>
		</user-attributes>
		<migration-details>
		</migration-details>
    </IDP>

	<IDP name="caGridDorianIDP">
		<class name="edu.wustl.authmanager.CAGridAuthManager" />
		<idpdetails>
			<defaultidp>false</defaultidp>
			<displayname>caGrid_Training</displayname>
		</idpdetails>
		<idp-configuration>
			<DorianServiceURL>https://cagrid-dorian.nci.nih.gov:8443/wsrf/services/cagrid/Dorian</DorianServiceURL>
			<securityAuthetication>simple</securityAuthetication>
			<securityProtocol>ssl</securityProtocol>
			<AuthenticationServiceURL>https://cagrid-dorian.nci.nih.gov:8443/wsrf/services/cagrid/Dorian</AuthenticationServiceURL>
			<SyncDescFilePath>/usr/local/catissue/sync-description.xml</SyncDescFilePath>
		</idp-configuration>
		<user-attributes>
		</user-attributes>
		<migration-details>
		</migration-details>
	</IDP>
</IDPS>
